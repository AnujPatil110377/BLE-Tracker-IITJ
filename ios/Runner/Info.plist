	<string>This app uses location to find nearby BLE devices in the background.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>This app uses location to find nearby BLE devices while the app is in use.</string>
    <key>UIBackgroundModes</key>
    <array>
        <string>bluetooth-central</string>
        <string>location</string>
    </array>
